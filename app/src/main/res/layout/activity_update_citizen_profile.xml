<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/userprof2"
    tools:context=".citizenProfile.SettingsCitizenProfile"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="-21dp">


    <TextView
        android:id="@+id/layoutdesign1"
        android:layout_width="388dp"
        android:layout_height="204dp"
        android:background="@drawable/update_profile_layout_design"
        android:contentDescription="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"/>


    <ImageView
        android:id="@+id/ProfilePhoto"
        android:layout_width="132dp"
        android:layout_height="131dp"
        android:background="@drawable/bubble"
        app:layout_constraintBottom_toBottomOf="@+id/layoutdesign1"
        app:layout_constraintEnd_toEndOf="@+id/layoutdesign1"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/layoutdesign1"
        app:layout_constraintTop_toTopOf="@+id/layoutdesign1"
        app:layout_constraintVertical_bias="0.301"
        app:srcCompat="@drawable/baseline_person_outline_24" />


    <TextView
        android:id="@+id/FullNameCitizen"
        android:layout_width="273dp"
        android:layout_height="24dp"
        android:layout_marginBottom="8dp"
        android:contentDescription="TextView"
        android:hint="Loading..."
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/layoutdesign1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ProfilePhoto"
        app:layout_constraintVertical_bias="0.789" />

    <TextView
        android:id="@+id/CitizenProfile"
        android:layout_width="336dp"
        android:layout_height="50dp"
        android:background="@drawable/emergency_contact"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layoutdesign1"
        app:layout_constraintVertical_bias="0.045" />

    <TextView
        android:id="@+id/EmergencyContactProfileButton"
        android:layout_width="189dp"
        android:layout_height="25dp"
        android:fontFamily="@font/archivo_black"
        android:text="User Profile"
        android:textSize="14sp"
        android:textStyle="bold"
        android:layout_marginLeft="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/CitizenProfile"
        app:layout_constraintStart_toEndOf="@+id/ProfileIcon"
        app:layout_constraintTop_toTopOf="@+id/CitizenProfile" />


    <ImageButton
        android:id="@+id/imageButtonforCitizenProfile"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:backgroundTint="#14F467"
        android:contentDescription="UserProfilenavigate"
        android:scrollbarThumbHorizontal="@null"
        android:scrollbarThumbVertical="@null"
        app:layout_constraintBottom_toBottomOf="@+id/CitizenProfile"
        app:layout_constraintEnd_toEndOf="@+id/CitizenProfile"
        app:layout_constraintHorizontal_bias="0.636"
        app:layout_constraintStart_toEndOf="@+id/EmergencyContactProfileButton"
        app:layout_constraintTop_toTopOf="@+id/CitizenProfile"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/baseline_navigate_next_24" />

    <TextView
        android:id="@+id/CitizenAddress"
        android:layout_width="336dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/emergency_contact"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CitizenProfile" />

    <ImageView
        android:id="@+id/ProfileIcon"
        android:layout_width="31dp"
        android:layout_height="28dp"
        android:layout_marginLeft="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/CitizenProfile"
        app:layout_constraintStart_toStartOf="@+id/CitizenProfile"
        app:layout_constraintTop_toTopOf="@+id/CitizenProfile"
        app:srcCompat="@drawable/user_username" />

    <ImageButton
        android:id="@+id/imageButtonforCitizenAddress"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:backgroundTint="#14F467"
        android:contentDescription="EmergencyContactnavigate"
        android:scrollbarThumbHorizontal="@null"
        android:scrollbarThumbVertical="@null"
        app:layout_constraintBottom_toBottomOf="@+id/CitizenAddress"
        app:layout_constraintEnd_toEndOf="@+id/CitizenAddress"
        app:layout_constraintStart_toEndOf="@+id/Show_Cit_Address"
        app:layout_constraintTop_toTopOf="@+id/CitizenAddress"
        app:srcCompat="@drawable/baseline_navigate_next_24" />

    <TextView
        android:id="@+id/CitizenEmergencyContact"
        android:layout_width="336dp"
        android:layout_height="48dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/emergency_contact"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CitizenAddress" />

    <TextView
        android:id="@+id/EmergencyContactCitizenButton"
        android:layout_width="189dp"
        android:layout_height="25dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/archivo_black"
        android:text="Emergency Contact"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/CitizenEmergencyContact"
        app:layout_constraintEnd_toEndOf="@+id/CitizenEmergencyContact"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="@+id/CitizenEmergencyContact" />

    <ImageButton
        android:id="@+id/imageButtonforEmergencyContact"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:backgroundTint="#14F467"
        android:contentDescription="EmergencyContactnavigate"
        android:scrollbarThumbHorizontal="@null"
        android:scrollbarThumbVertical="@null"
        app:layout_constraintBottom_toBottomOf="@+id/CitizenEmergencyContact"
        app:layout_constraintEnd_toEndOf="@+id/CitizenEmergencyContact"
        app:layout_constraintHorizontal_bias="0.625"
        app:layout_constraintStart_toEndOf="@+id/EmergencyContactCitizenButton"
        app:layout_constraintTop_toTopOf="@+id/CitizenEmergencyContact"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/baseline_navigate_next_24" />

    <TextView
        android:id="@+id/Show_Cit_Address"
        android:layout_width="200dp"
        android:layout_height="26dp"
        android:fontFamily="@font/archivo_black"
        android:text="Address"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/CitizenAddress"
        app:layout_constraintEnd_toEndOf="@+id/CitizenAddress"
        app:layout_constraintHorizontal_bias="0.139"
        app:layout_constraintStart_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/CitizenAddress"
        app:layout_constraintVertical_bias="0.541" />


    <Button
        android:id="@+id/back_to_user_profile_btn"
        android:layout_width="82dp"
        android:layout_height="48dp"
        android:background="@drawable/emergencybubble"
        android:text="Back"
        app:backgroundTint="@android:color/holo_green_light"
        app:layout_constraintBottom_toBottomOf="@+id/layoutdesign1"
        app:layout_constraintEnd_toEndOf="@+id/layoutdesign1"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="@+id/layoutdesign1"
        app:layout_constraintTop_toTopOf="@+id/layoutdesign1"
        app:layout_constraintVertical_bias="0.141" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="31dp"
        android:layout_height="28dp"
        android:layout_marginStart="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/CitizenAddress"
        app:layout_constraintStart_toStartOf="@+id/CitizenAddress"
        app:layout_constraintTop_toTopOf="@+id/CitizenAddress"
        app:srcCompat="@drawable/user_location" />

    <ProgressBar
        android:id="@+id/progressBarEditProfile"
        style="?android:attr/progressBarStyle"
        android:layout_width="109dp"
        android:layout_height="115dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:elevation="10dp"
        android:visibility="gone"/>

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="31dp"
        android:layout_height="28dp"
        android:layout_marginStart="12dp"
        android:backgroundTint="@color/material_dynamic_neutral60"
        app:layout_constraintBottom_toBottomOf="@+id/CitizenEmergencyContact"
        app:layout_constraintStart_toStartOf="@+id/CitizenEmergencyContact"
        app:layout_constraintTop_toTopOf="@+id/CitizenEmergencyContact"
        app:srcCompat="@drawable/user_emergency_contact" />

    <Button
        android:id="@+id/Update_Profile_Picture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:background="@drawable/bubble"
        android:text="Update Profile Picture"
        app:backgroundTint="@android:color/holo_green_light"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CitizenEmergencyContact"
        app:layout_constraintVertical_bias="0.0" />

    <ProgressBar
        android:id="@+id/progressBarSettingsCitizen"
        style="?android:attr/progressBarStyle"
        android:layout_width="109dp"
        android:layout_height="115dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:elevation="10dp"
        android:visibility="gone"/>





</androidx.constraintlayout.widget.ConstraintLayout>