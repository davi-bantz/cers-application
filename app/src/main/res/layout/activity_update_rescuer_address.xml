<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/userprof2"
    tools:context=".rescuerProfile.UpdateInfoRescuer.update_rescuer_address">

    <TextView
        android:id="@+id/Edit_Basic_Details"
        android:layout_width="241dp"
        android:layout_height="24dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/archivo_black"
        android:text="Update Address Details"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageButtonforbacktoSetting"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButtonforbacktoSetting"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="28dp"
        android:backgroundTint="#14F467"
        android:contentDescription="UserProfilenavigate"
        android:scrollbarThumbHorizontal="@null"
        android:scrollbarThumbVertical="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/user_backtosettings" />

    <TextView
        android:id="@+id/Update_Details"
        android:layout_width="334dp"
        android:layout_height="68dp"
        android:alpha="10.5"
        android:fontFamily="@font/alfa_slab_one"
        android:text="Update Address"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Edit_Basic_Details"
        app:layout_constraintVertical_bias="0.016" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="Purok"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Edit_Rescuer_Purok"
        app:layout_constraintEnd_toEndOf="@+id/Edit_Rescuer_Purok"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="@+id/Edit_Rescuer_Purok" />

    <EditText
        android:id="@+id/Edit_Rescuer_Purok"
        android:layout_width="336dp"
        android:layout_height="49dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/bubble"
        android:hint="Enter your Purok"
        android:padding="10dp"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Update_Details" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="Street"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Edit_Rescuer_Street"
        app:layout_constraintEnd_toEndOf="@+id/Edit_Rescuer_Street"
        app:layout_constraintHorizontal_bias="0.094"
        app:layout_constraintStart_toStartOf="@+id/Edit_Rescuer_Street" />

    <EditText
        android:id="@+id/Edit_Rescuer_Street"
        android:layout_width="336dp"
        android:layout_height="49dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/bubble"
        android:hint="Enter your Street"
        android:padding="10dp"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Edit_Rescuer_Purok" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Barangay"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Edit_Rescuer_Barangay"
        app:layout_constraintEnd_toEndOf="@+id/Edit_Rescuer_Barangay"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toStartOf="@+id/Edit_Rescuer_Barangay" />

    <EditText
        android:id="@+id/Edit_Rescuer_Barangay"
        android:layout_width="336dp"
        android:layout_height="49dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/bubble"
        android:hint="Enter your Barangay"
        android:padding="10dp"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Edit_Rescuer_Street" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="City"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Edit_Rescuer_City"
        app:layout_constraintEnd_toEndOf="@+id/Edit_Rescuer_City"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="@+id/Edit_Rescuer_City" />

    <EditText
        android:id="@+id/Edit_Rescuer_City"
        android:layout_width="336dp"
        android:layout_height="49dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/bubble"
        android:hint="Enter your City"
        android:padding="10dp"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Edit_Rescuer_Barangay" />


    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="Province"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Edit_Rescuer_Province"
        app:layout_constraintEnd_toEndOf="@+id/Edit_Rescuer_Province"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="@+id/Edit_Rescuer_Province" />

    <EditText
        android:id="@+id/Edit_Rescuer_Province"
        android:layout_width="336dp"
        android:layout_height="49dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/bubble"
        android:hint="Enter your Province"
        android:padding="10dp"
        android:textSize="12sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Edit_Rescuer_City" />

    <Button
        android:id="@+id/Update_AddResBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:background="@drawable/bubble"
        android:text="Update Address"
        app:backgroundTint="@android:color/holo_green_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Edit_Rescuer_Province" />

    <ProgressBar
        android:id="@+id/progressBar_UpdateADDRes"
        style="?android:attr/progressBarStyle"
        android:layout_width="109dp"
        android:layout_height="115dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:elevation="10dp"
        android:visibility="gone"/>

</androidx.constraintlayout.widget.ConstraintLayout>