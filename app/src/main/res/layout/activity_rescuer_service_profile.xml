<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/userprof2"
    tools:context=".rescuerProfile.Rescuer_ServiceProfile">

    <ImageButton
        android:id="@+id/imageButtonforbacktoSetting_SERProf"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:backgroundTint="#14F467"
        android:contentDescription="UserProfilenavigate"
        android:scrollbarThumbHorizontal="@null"
        android:scrollbarThumbVertical="@null"
        app:layout_constraintBottom_toTopOf="@+id/UserEmerConDetails"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.692"
        app:srcCompat="@drawable/user_backtosettings"/>

    <TextView
        android:id="@+id/Edit_Profile"
        android:layout_width="241dp"
        android:layout_height="24dp"
        android:layout_marginEnd="76dp"
        android:fontFamily="@font/archivo_black"
        android:text="Service Profile Details"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/UserEmerConDetails"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageButtonforbacktoSetting_SERProf"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.584" />

    <TextView
        android:id="@+id/UserEmerConDetails"
        android:layout_width="333dp"
        android:layout_height="116dp"
        android:alpha="10.5"
        android:fontFamily="@font/alfa_slab_one"
        android:text="Service Profile"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.109" />

    <TextView
        android:id="@+id/Rescuer_Organization"
        android:layout_width="321dp"
        android:layout_height="49dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/bubble"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/UserEmerConDetails" />

    <TextView
        android:id="@+id/Show_Rescuer_Organization"
        android:layout_width="250dp"
        android:layout_height="18dp"
        android:hint="Loading..."
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Rescuer_Organization"
        app:layout_constraintEnd_toEndOf="@+id/Rescuer_Organization"
        app:layout_constraintStart_toStartOf="@+id/Rescuer_Organization"
        app:layout_constraintTop_toTopOf="@+id/Rescuer_Organization" />

    <TextView
        android:id="@+id/Rescuer_Role"
        android:layout_width="321dp"
        android:layout_height="49dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/bubble"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Rescuer_Organization" />

    <TextView
        android:id="@+id/Show_Rescuer_Role"
        android:layout_width="250dp"
        android:layout_height="18dp"
        android:hint="Loading..."
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Rescuer_Role"
        app:layout_constraintEnd_toEndOf="@+id/Rescuer_Role"
        app:layout_constraintStart_toStartOf="@+id/Rescuer_Role"
        app:layout_constraintTop_toTopOf="@+id/Rescuer_Role" />

    <TextView
        android:id="@+id/Rescuer_Address_OS"
        android:layout_width="321dp"
        android:layout_height="49dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/bubble"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Rescuer_Role" />

    <TextView
        android:id="@+id/Show_AddressOS"
        android:layout_width="250dp"
        android:layout_height="18dp"
        android:hint="Loading..."
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Rescuer_Address_OS"
        app:layout_constraintEnd_toEndOf="@+id/Rescuer_Address_OS"
        app:layout_constraintStart_toStartOf="@+id/Rescuer_Address_OS"
        app:layout_constraintTop_toTopOf="@+id/Rescuer_Address_OS" />


    <RelativeLayout
        android:id="@+id/Rescuer_Basic_Skills"
        android:layout_width="313dp"
        android:layout_height="180dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/list_view_layout"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Rescuer_Address_OS">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:id="@+id/Basic_Skills"
            android:text="Basic Skills"
            android:textAlignment="center"
            android:textSize="20sp"
            android:textStyle="bold"
            android:paddingTop="5dp"
            android:fontFamily="@font/archivo_medium"
            />

        <TextView
            android:id="@+id/Show_Res_Basic_Skill"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:hint="Loading..."
            android:padding="20dp"
            android:textSize="12sp"
            android:textStyle="bold"
            android:layout_below="@id/Basic_Skills"/>

    </RelativeLayout>

    <Button
        android:id="@+id/Update_Service_Profile_Btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bubble"
        android:text="Update Service Profile"
        app:backgroundTint="@android:color/holo_green_light"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Rescuer_Basic_Skills"
        app:layout_constraintVertical_bias="0.205" />

    <ProgressBar
        android:id="@+id/progressBarService_Profile"
        style="?android:attr/progressBarStyle"
        android:layout_width="109dp"
        android:layout_height="115dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:elevation="10dp"
        android:visibility="gone"/>


</androidx.constraintlayout.widget.ConstraintLayout>