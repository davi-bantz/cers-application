        app:layout_constraintTop_toTopOf="@+id/EmergencyPurok" />

    <TextView
        android:id="@+id/EmergencyStreet"
        android:layout_width="321dp"
        android:layout_height="49dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/bubble"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EmergencyPurok" />

    <TextView
        android:id="@+id/Show_EmerStreet"
        android:layout_width="250dp"
        android:layout_height="18dp"
        android:hint="Loading..."
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/EmergencyStreet"
        app:layout_constraintEnd_toEndOf="@+id/EmergencyStreet"
        app:layout_constraintStart_toStartOf="@+id/EmergencyStreet"
        app:layout_constraintTop_toTopOf="@+id/EmergencyStreet" />

    <TextView
        android:id="@+id/EmergencyBarangay"
        android:layout_width="321dp"
        android:layout_height="49dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/bubble"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:backgroundTint="#9BFF8E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EmergencyStreet" />

    <TextView
        android:id="@+id/Show_EmerBarangay"
        android:layout_width="250dp"
        android:layout_height="18dp"
        android:hint="Loading..."
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/EmergencyBarangay"
