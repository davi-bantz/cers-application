        app:layout_constraintEnd_toEndOf="@+id/EmergencyProvince"
        app:layout_constraintStart_toStartOf="@+id/EmergencyProvince"
        app:layout_constraintTop_toTopOf="@+id/EmergencyProvince" />

    <Button
        android:id="@+id/ToUpdate_Cit_EmergencContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bubble"
        android:text="Save Changes"
        app:backgroundTint="@android:color/holo_green_light"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EmergencyProvince"
        app:layout_constraintVertical_bias="0.567" />

    <ProgressBar
        android:id="@+id/progressBarECCitUpdate"
        style="?android:attr/progressBarStyle"
        android:layout_width="109dp"
        android:layout_height="115dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:elevation="10dp"
        android:visibility="gone"/>


